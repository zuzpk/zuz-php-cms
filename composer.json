{
    "name": "zuz/framework",
    "type": "project",
    "description": "The skeleton application for the Zuz framework.",
    "license": "MIT",
    "authors": [
        {
            "name": "Kamran Wajdani",
            "email": "mkamranwajdani@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.8.1",
        "firebase/php-jwt": "^6.10",
        "ramsey/uuid": "^4.7",
        "voku/portable-ascii": "^2.0",
        "jbroadway/urlify": "^1.2",
        "vlucas/phpdotenv": "^5.6",        
        "hashids/hashids": "^5.0.2",
        "phpmailer/phpmailer": "^6.9.1"
    },
    "autoload": {
        "psr-4": {
            "Zuz\\" : "src/Zuz/",
            "App\\" : "src/App/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
